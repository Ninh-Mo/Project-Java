<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>Error</title>
</head>
<body>
<c:if test="${listError != 'empty' }">
    <h1>List Error</h1>
    <c:forEach items="${listError}" var="error">
        <h4>${error}</h4>
    </c:forEach>
</c:if>
<c:if test="${listCheck == 'empty'}">
    <h1>Error</h1>
</c:if>
</body>
</html>
